
typedef char TNombrej1[5];
typedef char TApellidoj1[10];


typedef struct{
TNombrej1 nombrej1;
TApellidoj1 apellidoj1;

}TJugadorj1;

TJugadorj1 jugador1;

printf("Introduzca su nombre (máximo 5 caracteres): ");
scanf("%s", jugador1.nombrej1);


printf("Introduzca su primer apellido (máximo 10 caracteres): ");
scanf("%s", jugador1.apellidoj1);

printf("Su nombre es %s y su apellido %s", jugador1.nombrej1, jugador1.apellidoj1)





